void main() {
  int x;
  int y;
  int z;

  x = -3;

  y = x++;
  printf("y=");
  printf("%d", y);
  printf("\n");

  z = !x;
  printf("z=");
  printf("%d", z);
  printf("\n");

  printf("z=");
  printf("%d", !z);
  printf("\n");

  printf("x=");
  printf("%d", x);
  printf("\n");

  printf("x=");
  printf("%d", x++);
  printf("\n");

  printf("x=");
  printf("%d", x);
  printf("\n");

  printf("x=");
  printf("%d", x--);
  printf("\n");

  printf("x=");
  printf("%d", x);
  printf("\n");

  printf("/////// ++, --, and arithmetic ////////\n");
  x = 1;
  y = 2;
  z = 0;
  printf("x=");
  printf("%d", x);
  printf(", y=");
  printf("%d", y);
  printf(", z=");
  printf("%d", z);
  printf("\n");

  z = x++ + y;
  printf("z = x++ + y;");
  printf("\n");

  printf("x=");
  printf("%d", x);
  printf(", y=");
  printf("%d", y);
  printf(", z=");
  printf("%d", z);
  printf("\n");

  z = x++ - y;
  printf("z = x++ - y;");
  printf("\n");

  printf("x=");
  printf("%d", x);
  printf(", y=");
  printf("%d", y);
  printf(", z=");
  printf("%d", z);
  printf("\n");

  z = x++ - y--;
  printf("z = x++ - y--;");
  printf("\n");

  printf("x=");
  printf("%d", x);
  printf(", y=");
  printf("%d", y);
  printf(", z=");
  printf("%d", z);
  printf("\n");

  z = x++ * (y++ + 1);
  printf("z = x++ * (y++ + 1);");
  printf("\n");

  printf("x=");
  printf("%d", x);
  printf(", y=");
  printf("%d", y);
  printf(", z=");
  printf("%d", z);
  printf("\n");

  z = x++ / y;
  printf("z = x++ / y;");
  printf("\n");

  printf("x=");
  printf("%d", x);
  printf(", y=");
  printf("%d", y);
  printf(", z=");
  printf("%d", z);
  printf("\n");

  printf("////// comparison ops //////////\n");
  x = 1;
  y = 2;
  z = 1;
  printf("x=");
  printf("%d", x);
  printf(", y=");
  printf("%d", y);
  printf(", z=");
  printf("%d", z);
  printf("\n");

  printf("x == y: ");
  printf("%d", x == y);
  printf("\n");

  printf("x == z: ");
  printf("%d", x == z);
  printf("\n");

  printf("x != y: ");
  printf("%d", x != y);
  printf("\n");

  printf("x != z: ");
  printf("%d", x != z);
  printf("\n");

  printf("!(x != z): ");
  printf("%d", !(x != z));
  printf("\n");

  printf("x < y: ");
  printf("%d", x < y);
  printf("\n");

  printf("x < z: ");
  printf("%d", x < z);
  printf("\n");

  printf("y > x: ");
  printf("%d", y > x);
  printf("\n");

  printf("x > z: ");
  printf("%d", x > z);
  printf("\n");

  printf("x <= z: ");
  printf("%d", x <= z);
  printf("\n");

  printf("y <= x: ");
  printf("%d", y <= x);
  printf("\n");

  printf("z >= x: ");
  printf("%d", z >= x);
  printf("\n");

  printf("x >= y: ");
  printf("%d", x >= y);
  printf("\n");

  printf("////// stmts other than call/return //////////\n");
  x = 2;
  printf("x=");
  printf("%d", x);
  printf("\n");

  x++;
  printf("x++; x=");
  printf("%d", x);
  printf("\n");

  x--;
  printf("x--; x=");
  printf("%d", x);
  printf("\n");

  ++x;
  printf("++x; x=");
  printf("%d", x);
  printf("\n");

  --x;
  printf("--x; x=");
  printf("%d", x);
  printf("\n");
}
