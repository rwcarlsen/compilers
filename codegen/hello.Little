void main() {
  int x;
  int y;
  int z;
  x = -3;
  y = x++;
  z = !x;

  printf("z=");
  printf("%d", z);
  printf("\n");

  printf("z=");
  printf("%d", !z);
  printf("\n");

  printf("x=");
  printf("%d", x);
  printf("\n");

  printf("x=");
  printf("%d", x++);
  printf("\n");

  printf("x=");
  printf("%d", x);
  printf("\n");

  printf("x=");
  printf("%d", x--);
  printf("\n");

  printf("x=");
  printf("%d", x);
  printf("\n");
}
